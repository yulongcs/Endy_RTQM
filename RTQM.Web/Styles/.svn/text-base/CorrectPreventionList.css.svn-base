/*标题*/
.CPL-title {
    height: 30px;line-height: 30px;font-weight: bold;font-size:14px
}
/*内容主体*/
.CPL-main {
    width: 920px;height: 450px;
    margin: 0px auto;
}
/*搜索区*/
.CPL-SearchArea {
    width: 100%;height: 30px;line-height: 30px;font-size: 12px;
    float: left
    
}
/*搜索区选项*/
.CPL-SearchItem {
    width: 150px;height: 30px;font-size: 12px;
    float: left;
}
/*搜索区日期控件Input*/
.CPL-PoDate {
    width: 80px; font-size: 12px;
}
/*搜索区按钮div*/
.CPL-select {
     width: 56px;height: 20px;
    float:left;margin-top: 3px;display: inline
}
/*文件内容列表div*/
.CPL-view {
    width:100%;height: 400px;overflow: auto;
    float: left;margin-top: 10px;display: inline;
}
/*列表table*/
.CPL-viewTable {
    width: 100%;
}
/*列名*/
.CPL-viewTable th {
    height: 25px;text-align:center; background: #099;font-size: 12px;
}
.CPL-viewTable td {
    height: 20px;font-size: 12px;text-align: left
}
/*内容列*/
.CPI-viewitem1 {
    width: 100px;
}
