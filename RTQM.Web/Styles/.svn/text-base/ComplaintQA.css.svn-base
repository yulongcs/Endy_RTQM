.CQA-title
{
    height: 30px;line-height: 30px;font-weight: bold;font-size:14px
}
.CQA-main 
{
    width:320px;height: 260px;border: solid 1px #60bfbf;
    margin: 50px auto;
}
.CQA-mainItem
{
    width: 300px;height: 30px;font-size: 12px;
    float: left; margin: 10px auto auto 30px;display: inline;
}
.CQA-ItemTitle {
    width: 100%;height: 30px;line-height: 30px;font-size: 12px;
    float: left;  margin: 10px auto auto 30px; display:inline
}
.CQA-ItemText {
    width: 250px;height: 120px;
    float: left; margin-left: 30px;display: inline;
}
.CQA-textarea {
    width: 250px;min-width: 250px; max-width: 250px;height: 100px;min-height: 110px; max-height: 110px;font-size: 12px;
}
.CQA-mainBts {
    width: 260px;height: 30px;
    margin: 20px auto;
}
.CQA-mainBtY 
{
    float: left;
    width: 56px;height: 30px;
}
.CQA-mainBtN 
{
    float:right;
    width: 56px;height: 30px;
}
