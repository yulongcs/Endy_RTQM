/*标题*/
.AC-title
{
    height: 30px;line-height: 30px;font-weight: bold;font-size:14px
}
/*内容主体*/
.AC-main 
{
    width:700px;height: 470px;border: solid 1px #60bfbf;
    margin: 5px auto;
}
/*内容选项*/
.AC-mainItem
{
    width: 300px;height: 25px;font-size: 12px;line-height: 25px;
    float: left; margin: 15px auto auto 50px;display: inline;
}
.AC-mainitemTitle {
    width: 75px;height: 25px;line-height: 25px;font-size: 12px;
    float: left;
}
/*textarea标题*/
.AC-ItemTitle {
    width: 100%;height: 30px;line-height: 30px;font-size: 12px;
    float: left;  margin-left:50px; display:inline
}
/*textarea外部DIV*/
.AC-ItemText {
    width: 250px;height: 45px;font-size: 12px;
   float: left;  margin-left:50px;display: inline;
}
/*textarea*/
.AC-Textarea {
    width: 580px;min-width: 580px; max-width: 580px;height: 40px;min-height: 40px; max-height: 40px;
}
/*内容部分按钮*/
.AC-mainBts {
    width: 500px;height: 30px;
    margin: 20px auto;
}
/*内容部分确认按钮*/
.AC-mainBtY 
{
    float: left;
    width: 56px;height: 30px;
}
/*内容部分取消按钮*/
.AC-mainBtN 
{
    float:right;
    width: 56px;height: 30px;
}
/*设置菜单关闭，当页面处于大菜单中，菜单打开*/
#TP-menuItemArea1
{
    display:block
}
/*设置当前页对应的菜单的样式*/
#AddPurchase{
    color: #099;
}